package loop;


public class MultiTable {

	public static void main(String[] args) {
		
		
		multiTable();


	}
	static void multiTable(){
		for(int i = 1; i < 100; i ++){
			System.out.print(i + "の段: ");
			for(int j = 1; j < 100; j ++){
				System.out.print(i * j + " ");
				
			}
			System.out.println();
		}
	}

}
