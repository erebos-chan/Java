package array;
import java.util.Scanner;

public class FindMin {

	public static void main(String[] args) {
		//スキャナの準備
		Scanner scan = new Scanner(System.in);
		
		//配列＆変数の宣言
		int num[] = new int[5];
		
		//プロンプトユーザ入力
		System.out.println("入力した数値の最小値を求めます。");
		for (int i = 0; i < 5; i++) {
			System.out.print("整数を入力");
			System.out.print(i + 1);
			System.out.print("番目>>");
			num[i] = scan.nextInt();
		}

	}

}
