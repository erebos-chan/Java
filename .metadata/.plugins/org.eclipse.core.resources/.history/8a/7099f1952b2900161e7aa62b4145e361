package loop;
import java.util.Scanner;


public class PrintGhar {

	public static void main(String[] args) {
		//スキャナの準備
		Scanner scan = new Scanner(System.in);
		
		//変数の宣言
		int time;
		
		//ユーザ入力
		System.out.print("整数を入力>>");
		time = scan.nextInt();
		
		printChar(time);
	}
	static void printChar(int time){
		for(int i  = time; i > 0; i--){
			System.out.print("*");
		}
	}

}
