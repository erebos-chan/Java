package array;

public class FindMax {

	public static void main(String[] args) {
		int max;
		int[] num = {20,35,2,78,5};
		
		max = findMax(num);
		
		System.out.println(max);

	}
	static int findMax(int num[]){
		int max = 0;
		
		for(int i = 0;i < num.length;i++){
			if(max < num[i]){
				max = num[i];
			}
		}
		
		return max;
	}

}
