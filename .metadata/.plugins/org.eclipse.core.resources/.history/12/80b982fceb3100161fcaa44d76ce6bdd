package array;

public class Array_avg {

	public static void main(String[] args) {
		int score[][] = new int[5][3];
		
		score[0][0] = 100;
		score[0][1] = 100;
		score[0][2] = 100;
		score[1][0] = 70;
		score[1][1] = 50;
		score[1][2] = 30;
		score[2][0] = 60;
		score[2][1] = 40;
		score[2][2] = 20;
		score[3][0] = 90;
		score[3][1] = 80;
		score[3][2] = 70;
		
		
		for(int i = 0 ; i < 5 ; i++){
			int sum = 0;
			int k = 0;
			for(; k < 3 ; k++){
				sum = sum + score[i][k];
				
			}
			score[4][i] = sum / 4;
		}
		System.out.println(score[4][0]);
		System.out.println(score[4][1]);
		System.out.println(score[4][2]);
	}

}
