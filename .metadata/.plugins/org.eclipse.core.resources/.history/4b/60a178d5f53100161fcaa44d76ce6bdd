package array;

public class Array_F {

	public static void main(String[] args) {

		String alph[][] = { 
				{ "　", "●", "●", "●", "　" },
				{ "　", "●", "　", "　", "　" },
				{ "　", "●", "●", "●", "　" },
				{ "　", "●", "　", "　", "　" },
				{ "　", "●", "　", "　", "　" }, 
				};
	
		
		String temp;

		for (int i = 0; i < 5; i++) {
			int j = 0;
			int k = 4;
			for (; j == k; j++) {
				temp = alph[i][j];
				alph[i][j] = alph[i][k];
				alph[i][k] = temp;
				k = k - 1;
			}

		for (int p = 0; p < 5; p++) {
			int q = 0;	
			System.out.print(alph[p][q]);
			q = q + 1;
				}
		System.out.println();

			}

		}
	}

	
